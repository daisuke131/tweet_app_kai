<div class="main users-new">
  <div class="container">
    <div class="form-heading">ログイン</div>
    <div class="form users-form">
      <div class="form-body">
        <%= form_for(@user) do |f| %>

        <p>メールアドレス</p>
        <%= f.text_field :email %>
        <p>パスワード</p>
        <%= f.password_field :password %>
        <%= f.submit %>
        <% end %>
      </div>
    </div>
  </div>
</div>